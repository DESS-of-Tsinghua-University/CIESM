./build-namelist:3487:
./build-namelist:3488:sub check_input_files {
./build-namelist:3489:
./build-namelist:3490:# For each variable in the namelist which is an input dataset, or contains filepaths
./build-namelist:3491:# for input datasets, either:
./build-namelist:3492:# 1) check to see if the input dataset exists on local disk, or
./build-namelist:3493:# 2) write the input dataset filepath to a file.
./build-namelist:3494:#
./build-namelist:3495:# In "test" mode (1) the routine is called with just one argument.
./build-namelist:3496:# In "inputdata" mode (2) the routine is called with three arguments.
./build-namelist:3497:#
./build-namelist:3498:# ***** N.B. ***** This routine assumes the following variables are in package main::
./build-namelist:3499:#  $definition        -- the namelist definition object
./build-namelist:3500:
./build-namelist:3501:    my $nl                = shift;    # namelist object
./build-namelist:3502:    my $inputdata_rootdir = shift;    # if false prints test, else creates inputdata file
./build-namelist:3503:    my $outfile           = shift;
./build-namelist:3504:
./build-namelist:3505:    my $fh;
./build-namelist:3506:    if (defined $inputdata_rootdir) {
./build-namelist:3507:	open $fh, '>', $outfile or die "check_input_files: error opening $outfile: $!";
./build-namelist:3508:    }
./build-namelist:3509:
./build-namelist:3510:    # Look through all namelist groups
./build-namelist:3511:    my @groups = $nl->get_group_names();
./build-namelist:3512:    foreach my $group (@groups) {
./build-namelist:3513:
./build-namelist:3514:	# Look through all variables in each group
./build-namelist:3515:	my @vars = $nl->get_variable_names($group);
./build-namelist:3516:	foreach my $var (@vars) {
./build-namelist:3517:
./build-namelist:3518:	    # Is the variable an input dataset?
./build-namelist:3519:	    my $input_pathname_type = $definition->is_input_pathname($var);
./build-namelist:3520:
./build-namelist:3521:	    # If it is, check whether it exists locally and print status
./build-namelist:3522:	    if ($input_pathname_type) {
./build-namelist:3523:
./build-namelist:3524:		# Get pathname of input dataset
./build-namelist:3525:		my $pathname = $nl->get_variable_value($group, $var);
./build-namelist:3526:		# Need to strip the quotes
./build-namelist:3527:		$pathname =~ s/['"]//g;
./build-namelist:3528:
./build-namelist:3529:		if ($input_pathname_type eq 'abs') {
./build-namelist:3530:                    if ($inputdata_rootdir) {
./build-namelist:3531:                        print $fh "$var = $pathname\n";
./build-namelist:3532:                    }
./build-namelist:3533:                    else {
./build-namelist:3534:		        if (-e $pathname) {  # use -e rather than -f since the absolute pathname
./build-namelist:3535:			                     # might be a directory
./build-namelist:3536:			    print "OK -- found $var = $pathname\n";
./build-namelist:3537:		        }
./build-namelist:3538:		        else {
./build-namelist:3539:			    print "NOT FOUND:  $var = $pathname\n";
./build-namelist:3540:		        }
./build-namelist:3541:                    }
./build-namelist:3542:		}
./build-namelist:3543:		elsif ($input_pathname_type =~ m/rel:(.+)/o) {
./build-namelist:3544:		    # The match provides the namelist variable that contains the
./build-namelist:3545:		    # root directory for a relative filename
./build-namelist:3546:		    my $rootdir_var = $1;
./build-namelist:3547:		    my $rootdir = $nl->get_variable_value($group, $rootdir_var);
./build-namelist:3548:		    $rootdir =~ s/['"]//g;          #"'
./build-namelist:3549:                    if ($inputdata_rootdir) {
./build-namelist:3550:                        $pathname = "$rootdir/$pathname";
./build-namelist:3551:                        print $fh "$var = $pathname\n";
./build-namelist:3552:                    }
./build-namelist:3553:                    else {
./build-namelist:3554:		        if (-f "$rootdir/$pathname") {
./build-namelist:3555:			    print "OK -- found $var = $rootdir/$pathname\n";
./build-namelist:3556:		        }
./build-namelist:3557:		        else {
./build-namelist:3558:			    print "NOT FOUND:  $var = $rootdir/$pathname\n";
./build-namelist:3559:		        }
./build-namelist:3560:                    }
./build-namelist:3561:		}
./build-namelist:3562:	    }
./build-namelist:3563:	}
./build-namelist:3564:    }
./build-namelist:3565:
./build-namelist:3566:    # Treat special cases...
./build-namelist:3567:
./build-namelist:3568:    # These namelist variables are arrays of strings.
./build-namelist:3569:    my @vars = qw(ext_frc_specifier srf_emis_specifier mode_defs rad_climate rad_diag_1 rad_diag_2
./build-namelist:3570:                  rad_diag_3 rad_diag_4 rad_diag_5 rad_diag_6 rad_diag_7 rad_diag_8
./build-namelist:3571:                  rad_diag_9 rad_diag_10);
./build-namelist:3572:    foreach my $var (@vars) {
./build-namelist:3573:
