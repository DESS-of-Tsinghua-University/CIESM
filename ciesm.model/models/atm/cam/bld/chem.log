./perl5lib/XML/Lite/Element.pm:294:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:295:# Date      Modification                              Author
./perl5lib/XML/Lite/Element.pm:296:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:297:# 04Nov2001 Added function to get text                   JW
./perl5lib/XML/Lite/Element.pm:298:# 06Nov2001 Added <.../> optimization                    JW
./perl5lib/XML/Lite/Element.pm:299:# 06Nov2001 Included CDATA text recovery                 JW
./perl5lib/XML/Lite/Element.pm:300:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:301:sub text;
./perl5lib/XML/Lite/Element.pm:302:*text = \&get_text;
./perl5lib/XML/Lite/Element.pm:303:sub get_text {
./perl5lib/XML/Lite/Element.pm:304:	my $self = shift;
./perl5lib/XML/Lite/Element.pm:305:	my $content = '';
./perl5lib/XML/Lite/Element.pm:306:
./perl5lib/XML/Lite/Element.pm:307:	# If we don't have any content, then we should return  
./perl5lib/XML/Lite/Element.pm:308:	# $content right away -- we have no text
./perl5lib/XML/Lite/Element.pm:309:	return $content unless defined $self->{node}[2];
./perl5lib/XML/Lite/Element.pm:310:
./perl5lib/XML/Lite/Element.pm:311:	# Otherwise get out content and children
./perl5lib/XML/Lite/Element.pm:312:	my @children = $self->get_children;
./perl5lib/XML/Lite/Element.pm:313:	my $orig_content = $self->get_content;
./perl5lib/XML/Lite/Element.pm:314:	
./perl5lib/XML/Lite/Element.pm:315:	# Then remove the child elements from our content
./perl5lib/XML/Lite/Element.pm:316:	my $start = 0;
./perl5lib/XML/Lite/Element.pm:317:	foreach( @children ) {
./perl5lib/XML/Lite/Element.pm:318:		my $end = $_->{node}[0] - $self->{node}[1] - 1;
./perl5lib/XML/Lite/Element.pm:319:		$content .= substr( $orig_content, $start, $end - $start);
./perl5lib/XML/Lite/Element.pm:320:		$start = ($_->{node}[3] || $_->{node}[1]) - $self->{node}[1];
./perl5lib/XML/Lite/Element.pm:321:	} # end foreach
./perl5lib/XML/Lite/Element.pm:322:	$content .= substr( $orig_content, $start ) if $start < length($orig_content);
./perl5lib/XML/Lite/Element.pm:323:	
./perl5lib/XML/Lite/Element.pm:324:	# Remove the CDATA wrapper, preserving the content
./perl5lib/XML/Lite/Element.pm:325:	$content =~ s/<!\[CDATA\[(.+?)]\]>/$1/g;
./perl5lib/XML/Lite/Element.pm:326:	
./perl5lib/XML/Lite/Element.pm:327:	# Return the left-over text
./perl5lib/XML/Lite/Element.pm:328:	return $content;
./perl5lib/XML/Lite/Element.pm:329:} # end get_text
