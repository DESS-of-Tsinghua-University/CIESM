./perl5lib/XML/Lite/Element.pm:255:sub get_children {
./perl5lib/XML/Lite/Element.pm:256:	my $self = shift;
./perl5lib/XML/Lite/Element.pm:257:	my @children = ();
./perl5lib/XML/Lite/Element.pm:258:
./perl5lib/XML/Lite/Element.pm:259:	# If we don't have any content, then we should return an emtpty 
./perl5lib/XML/Lite/Element.pm:260:	# list right away -- we have no children.
./perl5lib/XML/Lite/Element.pm:261:	return @children unless defined $self->{node}[2];
./perl5lib/XML/Lite/Element.pm:262:
./perl5lib/XML/Lite/Element.pm:263:	# We need to traverse the document tree and find our own node
./perl5lib/XML/Lite/Element.pm:264:	# This will also load {children} and {parent} as well
./perl5lib/XML/Lite/Element.pm:265:	$self->_find_self() unless defined $self->{self};
./perl5lib/XML/Lite/Element.pm:266:
./perl5lib/XML/Lite/Element.pm:267:	# Now that we know who we are (if this didn't fail) we can 
./perl5lib/XML/Lite/Element.pm:268:	# iterate through the sub nodes (our child list) and make 
./perl5lib/XML/Lite/Element.pm:269:	# XML::Lite::Elements objects for each child
./perl5lib/XML/Lite/Element.pm:270:	if( defined $self->{children} ) {
./perl5lib/XML/Lite/Element.pm:271:		my $i = 0;
./perl5lib/XML/Lite/Element.pm:272:		my $node = $self->{children}[$i];
./perl5lib/XML/Lite/Element.pm:273:		while( defined $node ) {
./perl5lib/XML/Lite/Element.pm:274:			push @children, XML::Lite::Element->new( $self->{doc}, $node );
./perl5lib/XML/Lite/Element.pm:275:			$i++ if (@$node == 4) && (defined $node->[2]); # Skip element's child list if it exists
./perl5lib/XML/Lite/Element.pm:276:			$node = $self->{children}[++$i];
./perl5lib/XML/Lite/Element.pm:277:		} # end while
./perl5lib/XML/Lite/Element.pm:278:	} # end if
./perl5lib/XML/Lite/Element.pm:279:	
./perl5lib/XML/Lite/Element.pm:280:	return @children;
./perl5lib/XML/Lite/Element.pm:281:} # end get_children
