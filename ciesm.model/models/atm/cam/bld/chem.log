./mkDepends:412:#--------------------------------------------------------------------------------------
./mkDepends:413:
./mkDepends:414:sub usage {
./mkDepends:415:    ($ProgName = $0) =~ s!.*/!!;            # name of program
./mkDepends:416:    die <<EOF
./mkDepends:417:SYNOPSIS
./mkDepends:418:     $ProgName [-d depfile] [-m mangle_scheme] [-t dir] [-w] Filepath Srcfiles
./mkDepends:419:OPTIONS
./mkDepends:420:     -d depfile
./mkDepends:421:          Additional file to be added to every .o dependence.
./mkDepends:422:     -m mangle_scheme
./mkDepends:423:          Method of mangling Fortran module names into .mod filenames.
./mkDepends:424:          Allowed values are:
./mkDepends:425:              lower - Filename is module_name.mod
./mkDepends:426:              upper - Filename is MODULE_NAME.MOD
./mkDepends:427:          The default is -m lower.
./mkDepends:428:     -t dir
./mkDepends:429:          Target directory.  If this option is set the .o files that are
./mkDepends:430:          targets in the dependency rules have the form dir/file.o.
./mkDepends:431:     -w   Print warnings to STDERR about files or dependencies not found.
./mkDepends:432:ARGUMENTS
./mkDepends:433:     Filepath is the name of a file containing the directories (one per 
./mkDepends:434:     line) to be searched for dependencies.  Srcfiles is the name of a
./mkDepends:435:     file containing the names of files (one per line) for which
./mkDepends:436:     dependencies will be generated.
./mkDepends:437:EOF
./mkDepends:438:}
./mkSrcfiles:1:#!/usr/bin/env perl
./mkSrcfiles:2:
./mkSrcfiles:3:# Make list of files containing source code.  The source list contains all
./mkSrcfiles:4:# .F90, .f90, .F, and .c files in a specified list of directories.  The
./mkSrcfiles:5:# directories are specified one per line in a file called Filepath which
./mkSrcfiles:6:# this script tries to open in the current directory.  The current
./mkSrcfiles:7:# directory is prepended to the specified list of directories.  If Filepath
./mkSrcfiles:8:# doesn't exist then only the source files in the current directory are
./mkSrcfiles:9:# listed.
./mkSrcfiles:10:# The list of source files is written to the file Srcfiles.
./mkSrcfiles:11:
./mkSrcfiles:12:# Check usage:
./mkSrcfiles:13:@ARGV == 0                 or usage();
./mkSrcfiles:14:
./mkSrcfiles:15:open(SRC,"> Srcfiles")     or die "Can't open Srcfiles\n";
./mkSrcfiles:16:
./mkSrcfiles:17:if ( open(FILEPATH,"< Filepath") ) {
./mkSrcfiles:18:    @paths = <FILEPATH>;
./mkSrcfiles:19:} else {
./mkSrcfiles:20:    @paths = ();
./mkSrcfiles:21:}
./mkSrcfiles:22:chomp @paths;
./mkSrcfiles:23:unshift(@paths, '.');
./mkSrcfiles:24:foreach $dir (@paths) {  # (could check that directories exist here)
./mkSrcfiles:25:    $dir =~ s!/?\s*$!!;  # remove / and any whitespace at end of directory name
./mkSrcfiles:26:    ($dir) = glob $dir;  # Expand tildes in path names.
./mkSrcfiles:27:}
./mkSrcfiles:28:
./mkSrcfiles:29:# Loop through the directories and add each filename as a hash key.  This
./mkSrcfiles:30:# automatically eliminates redunancies.
./mkSrcfiles:31:%src = ();
./mkSrcfiles:32:foreach $dir (@paths) {
./mkSrcfiles:33:    @filenames = (glob("$dir/*.[Fc]"), glob("$dir/*.[Ff]90"));
./mkSrcfiles:34:    foreach $filename (@filenames) {
./mkSrcfiles:35:	$filename =~ s!.*/!!;                   # remove part before last slash
./mkSrcfiles:36:	$src{$filename} = "";
./mkSrcfiles:37:    }
./mkSrcfiles:38:}
./mkSrcfiles:39:
./mkSrcfiles:40:foreach $file ( sort keys %src ) {
./mkSrcfiles:41:  print SRC "$file\n";
./mkSrcfiles:42:}
./mkSrcfiles:43:
./mkSrcfiles:44:#--------------------------------------------------------------------------------------
./mkSrcfiles:45:
./mkSrcfiles:46:sub usage {
./mkSrcfiles:47:    ($ProgName = $0) =~ s!.*/!!;            # name of program
./mkSrcfiles:48:    die <<EOF
./mkSrcfiles:49:SYNOPSIS
./mkSrcfiles:50:     $ProgName
./mkSrcfiles:51:DESCRIPTION
./mkSrcfiles:52:     The $ProgName utility assumes the existence of an input file
./mkSrcfiles:53:     ./Filepath, and writes an output file ./Srcfiles that contains
./mkSrcfiles:54:     the names of all the files that match the patterns *.F90, *.F,
./mkSrcfiles:55:     and *.c in all the directories from ./Filepath plus ./.  The
./mkSrcfiles:56:     files are listed one per line.
./mkSrcfiles:57:EOF
./mkSrcfiles:58:}
