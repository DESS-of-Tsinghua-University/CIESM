./perl5lib/XML/Lite/Element.pm:336:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:337:# Sub: _parse_attrs
./perl5lib/XML/Lite/Element.pm:338:#
./perl5lib/XML/Lite/Element.pm:339:# Args: (None)
./perl5lib/XML/Lite/Element.pm:340:#
./perl5lib/XML/Lite/Element.pm:341:# Returns: True value on success, false on failure
./perl5lib/XML/Lite/Element.pm:342:#
./perl5lib/XML/Lite/Element.pm:343:# Description: Pares the attributes in the element into a hash
./perl5lib/XML/Lite/Element.pm:344:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:345:# Date      Modification                              Author
./perl5lib/XML/Lite/Element.pm:346:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:347:# 18Nov2006 Allow whitespace between = and attribute     BEE
./perl5lib/XML/Lite/Element.pm:348:#           value.  Allow values to use either single
./perl5lib/XML/Lite/Element.pm:349:#           or double quotes.
./perl5lib/XML/Lite/Element.pm:350:# 08Apr2002 Allow null strings as valid values           BEE
./perl5lib/XML/Lite/Element.pm:351:# 13Mar2002 Don't do anything if not defined             EBK
./perl5lib/XML/Lite/Element.pm:352:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:353:sub _parse_attrs {
./perl5lib/XML/Lite/Element.pm:354:	my $self = shift;
./perl5lib/XML/Lite/Element.pm:355:	
./perl5lib/XML/Lite/Element.pm:356:	my $attrs = $self->{_attrs};
./perl5lib/XML/Lite/Element.pm:357:	if ( defined($attrs) ) {
./perl5lib/XML/Lite/Element.pm:358:		$attrs =~ s/^\s+//;
./perl5lib/XML/Lite/Element.pm:359:		$attrs =~ s/\s+$//;
./perl5lib/XML/Lite/Element.pm:360:		$self->{attrs} = {};
./perl5lib/XML/Lite/Element.pm:361:		while( $attrs =~ s/^(\S+)\s*=\s*["']([^"]*)["']// )    #" For syntax highlighter
./perl5lib/XML/Lite/Element.pm:362:		{
./perl5lib/XML/Lite/Element.pm:363:			$self->{attrs}{$1} = $2;
./perl5lib/XML/Lite/Element.pm:364:			$attrs =~ s/^\s+//;
./perl5lib/XML/Lite/Element.pm:365:		} # end while
./perl5lib/XML/Lite/Element.pm:366:	}
./perl5lib/XML/Lite/Element.pm:367:	
./perl5lib/XML/Lite/Element.pm:368:	return 1;
./perl5lib/XML/Lite/Element.pm:369:} # end _parse_atttrs
