./perl5lib/Build/NamelistDefinition.pm:187:sub validate
./perl5lib/Build/NamelistDefinition.pm:188:{
./perl5lib/Build/NamelistDefinition.pm:189:
./perl5lib/Build/NamelistDefinition.pm:190:# Validate a namelist object (created by the Build::Namelist module)
./perl5lib/Build/NamelistDefinition.pm:191:# against a namelist definition file.  All variables are checked and
./perl5lib/Build/NamelistDefinition.pm:192:# problems reported to stdout.  If any problems were encountered the object
./perl5lib/Build/NamelistDefinition.pm:193:# will throw an exception.
./perl5lib/Build/NamelistDefinition.pm:194:#
./perl5lib/Build/NamelistDefinition.pm:195:# If all variables are successfully validated, then the returned namelist
./perl5lib/Build/NamelistDefinition.pm:196:# object will have all variables in the namelist group(s) that are
./perl5lib/Build/NamelistDefinition.pm:197:# specified in the definition file.  This means that the namelist groups
./perl5lib/Build/NamelistDefinition.pm:198:# in the input namelist object are ignored.
./perl5lib/Build/NamelistDefinition.pm:199:
./perl5lib/Build/NamelistDefinition.pm:200:    my $self  = shift;
./perl5lib/Build/NamelistDefinition.pm:201:    my $nl    = shift;    # namelist to be validated
./perl5lib/Build/NamelistDefinition.pm:202:    
./perl5lib/Build/NamelistDefinition.pm:203:    # Create an empty namelist which will be populated with variables from the
./perl5lib/Build/NamelistDefinition.pm:204:    # input namelist as they are validated.
./perl5lib/Build/NamelistDefinition.pm:205:    my $nl_valid = Build::Namelist->new();
./perl5lib/Build/NamelistDefinition.pm:206:
./perl5lib/Build/NamelistDefinition.pm:207:    # Loop over the groups in the input namelist
./perl5lib/Build/NamelistDefinition.pm:208:    my @groups = $nl->get_group_names();
./perl5lib/Build/NamelistDefinition.pm:209:    for my $group (@groups) {
./perl5lib/Build/NamelistDefinition.pm:210:
./perl5lib/Build/NamelistDefinition.pm:211:	# Loop over the variables in the namelist group
./perl5lib/Build/NamelistDefinition.pm:212:	my @vars = $nl->get_variable_names($group);
./perl5lib/Build/NamelistDefinition.pm:213:	for my $var (@vars) {
./perl5lib/Build/NamelistDefinition.pm:214:
./perl5lib/Build/NamelistDefinition.pm:215:	    # Get the variable's value
./perl5lib/Build/NamelistDefinition.pm:216:	    my $value = $nl->get_variable_value($group, $var);
./perl5lib/Build/NamelistDefinition.pm:217:
./perl5lib/Build/NamelistDefinition.pm:218:	    # Validate the variable/value pair.  This method throws an exception
./perl5lib/Build/NamelistDefinition.pm:219:	    # when an error is encountered.  If the validation is successful, then
./perl5lib/Build/NamelistDefinition.pm:220:	    # the valid group name for the variable is returned.
./perl5lib/Build/NamelistDefinition.pm:221:	    my $valid_group = $self->_validate_pair($var, $value);
./perl5lib/Build/NamelistDefinition.pm:222:
./perl5lib/Build/NamelistDefinition.pm:223:	    # Add the validated variable to the output namelist
./perl5lib/Build/NamelistDefinition.pm:224:	    $nl_valid->set_variable_value($valid_group, $var, $value);
./perl5lib/Build/NamelistDefinition.pm:225:	}
./perl5lib/Build/NamelistDefinition.pm:226:    }
./perl5lib/Build/NamelistDefinition.pm:227:
./perl5lib/Build/NamelistDefinition.pm:228:    return $nl_valid;
./perl5lib/Build/NamelistDefinition.pm:229:}
