./run-scam.csh:80:
./run-scam.csh:81:set SCAM_MODS = $WRKDIR/$CASE/mods    && if (! -e  $SCAM_MODS) mkdir -p $SCAM_MODS 
./run-scam.csh:82:rm -rf $SCAM_MODS/*
./run-scam.csh:83:/bin/cp -f $USR_SRC/* $SCAM_MODS
./run-scam.csh:84:
./run-scam.csh:85:set BLDDIR    = $WRKDIR/$CASE/{$CASE}_bld_L${levarr}_${aero_mode}  && if (! -e  $BLDDIR) mkdir -p $BLDDIR
./run-scam.csh:86:cd $BLDDIR
./run-scam.csh:87:
./run-scam.csh:88:set IOPDESC = `grep IOP\: $CAM_ROOT/models/atm/cam/bld/namelist_files/use_cases/scam_${iopname}.xml`
./run-scam.csh:89:
./run-scam.csh:90:echo ""
./run-scam.csh:91:echo "***** $IOPDESC *****"
./run-scam.csh:92:echo ""
./run-scam.csh:93:
./run-scam.csh:94:##------------------------------------------------
./run-scam.csh:95:## Configure for building
./run-scam.csh:96:##------------------------------------------------
./run-scam.csh:97:   
./run-scam.csh:98:$CAM_ROOT/models/atm/cam/bld/configure -s -chem $aero_mode -nlev $levarr -dyn eul -res 64x128 -nospmd -nosmp -cppdefs -DDISABLE_TIMERS -scam -usr_src $SCAM_MODS -fc $USER_FC $DBUG -ldflags "-llapack -lblas -Mnobounds" -cice_nx 1 -cice_ny 1 #-microphys mg1.5
./run-scam.csh:99:
./run-scam.csh:100:##--------------------------
./run-scam.csh:101:## compile
./run-scam.csh:102:##--------------------------
./run-scam.csh:103:
./run-scam.csh:104:echo ""
./run-scam.csh:105:echo " -- Compile"
./run-scam.csh:106:echo ""
./run-scam.csh:107:gmake -j >&! MAKE.out || echo "ERROR: Compile failed for' bld_${levarr}_${aero_mode} - exiting run_scam" && exit 1
./run-scam.csh:108:
./run-scam.csh:109:#--------------------------
./run-scam.csh:110:## Build the namelist with extra fields needed for scam diagnostics
./run-scam.csh:111:##--------------------------
./run-scam.csh:112:
./run-scam.csh:113:cat <<EOF >! tmp_namelistfile
./run-scam.csh:114:&camexp 
./run-scam.csh:115:    history_budget       = .true.,
./run-scam.csh:116:    dtime                = $tarray,
./run-scam.csh:117:    ndens                = 1,
./run-scam.csh:118:    fincl1               = 'CLDST','ZMDLF','ZMDT','ZMDQ',
./run-scam.csh:119:                          'ICWMR','ICIMR','FREQL','FREQI','LANDFRAC','CDNUMC','FICE','WSUB','CCN3','ICLDIWP',
./run-scam.csh:120:                          'CDNUMC', 'AQSNOW',  'WSUB', 'CCN3', 'FREQI', 'FREQL', 'FREQR', 'FREQS', 'CLDLIQ', 'CLDICE', 
./run-scam.csh:121:                          'FSDS', 'FLDS','AREL','AREI','NSNOW','QSNOW','DSNOW','AWNC','AWNI',  
./run-scam.csh:122:                          'FLNT','FLNTC','FSNT','FSNTC','FSNS','FSNSC','FLNT','FLNTC','QRS','QRSC','QRL','QRLC',
./run-scam.csh:123:                          'LWCF','SWCF', 'NCAI', 'NCAL', 'NIHF','NIDEP','NIIMM','NIMEY', 'ICLDIWP','ICLDTWP', 'CONCLD', 
./run-scam.csh:124:                          'QCSEVAP', 'QISEVAP', 'QVRES', 'CMELIQ', 'CMEIOUT', 'EVAPPREC', 'EVAPSNOW', 'TAQ', 
./run-scam.csh:125:                          'ICLMRCU', 'ICIMRCU' ,'ICWMRSH' ,'ICWMRDP', 'ICLMRTOT' , 'ICIMRTOT' , 'SH_CLD' ,  'DP_CLD', 
./run-scam.csh:126:                          'LIQCLDF','ICECLDF', 'ICWMRST', 'ICIMRST', 'EFFLIQ', 'EFFICE','ADRAIN','ADSNOW'
./run-scam.csh:127:/
./run-scam.csh:128:EOF
./run-scam.csh:129:
./run-scam.csh:130:
./run-scam.csh:131:$CAM_ROOT/models/atm/cam/bld/build-namelist -s -runtype startup -infile tmp_namelistfile -use_case scam_${iopname} -csmdata $CSMDATA \
./run-scam.csh:132:    || echo "build-namelist failed" && exit 1
./run-scam.csh:133:
./run-scam.csh:134:set RUNDIR    = $WRKDIR/$CASE/$EXPNAME                  && if (! -e  $RUNDIR) mkdir -p $RUNDIR
./run-scam.csh:135:cd $RUNDIR
./run-scam.csh:136:
./run-scam.csh:137: ### RUN
./run-scam.csh:138:
./run-scam.csh:139:cp -f $BLDDIR/docn.stream.txt $RUNDIR
./run-scam.csh:140:cp -f $BLDDIR/*_in            $RUNDIR
./run-scam.csh:141:cp -f $BLDDIR/cam             $RUNDIR
./run-scam.csh:142:
./run-scam.csh:143:echo ""
./run-scam.csh:144:echo " -- Running SCAM in $RUNDIR"
./run-scam.csh:145:echo ""
./run-scam.csh:146:./cam >&! scam_output.txt
./run-scam.csh:147:
./run-scam.csh:148:end           #foreach iopname
./run-scam.csh:149:end           #foreach tarray
./run-scam.csh:150:end           #foreach levarr
./run-scam.csh:151:
./run-scam.csh:152:exit 0
./run-yellowstone.csh:1:#! /bin/csh -f
./run-yellowstone.csh:2:
./run-yellowstone.csh:3:##=======================================================================
./run-yellowstone.csh:4:##
./run-yellowstone.csh:5:##  run-yellowston.csh
./run-yellowstone.csh:6:##
./run-yellowstone.csh:7:##  Generic batch submission script for yellowstone using LSF.  
./run-yellowstone.csh:8:##
./run-yellowstone.csh:9:##-----------------------------------------------------------------------
./run-yellowstone.csh:10:## Usage for intel compiler (default): 
./run-yellowstone.csh:11:##   ./run_yellowstone.csh build
./run-yellowstone.csh:12:##   bsub < run-yellowstone.csh
./run-yellowstone.csh:13:##-----------------------------------------------------------------------
./run-yellowstone.csh:14:##
./run-yellowstone.csh:15:## This is an example script to build and run the default CAM configuration
./run-yellowstone.csh:16:## on Yellowstone.  The default configuration is 1.9x2.5L26, Finite-Volume 
./run-yellowstone.csh:17:## dynamics, CLM2 land model, CICE ice model, and CAM data ocean model.  
./run-yellowstone.csh:18:## Script will request 1 node to run with 16 MPI tasks and 2 SMP threads/task.
./run-yellowstone.csh:19:##
./run-yellowstone.csh:20:## Setting LSF options for batch queue submission.
./run-yellowstone.csh:21:#BSUB -a poe                     # use LSF openmp elim
./run-yellowstone.csh:22:#BSUB -x                         # exclusive use of node (not_shared)
./run-yellowstone.csh:23:#BSUB -n 16                      # yellowstone setting
./run-yellowstone.csh:24:#BSUB -R "span[ptile=16]"        # yellowstone setting
./run-yellowstone.csh:25:#BSUB -o out.%J                  # output filename
./run-yellowstone.csh:26:#BSUB -e out.%J                  # error filename
./run-yellowstone.csh:27:#BSUB -q small                   # queue
./run-yellowstone.csh:28:#BSUB -W 0:10                    # wall clock limit
./run-yellowstone.csh:29:# #BSUB -P <please_set>            # account number 
./run-yellowstone.csh:30:
./run-yellowstone.csh:31:if ($#argv == 1 && $1 != "build") then
./run-yellowstone.csh:32:  echo "Invalid command line option: $1 "
./run-yellowstone.csh:33:  exit
./run-yellowstone.csh:34:endif
./run-yellowstone.csh:35:
./run-yellowstone.csh:36:##set number of tasks
./run-yellowstone.csh:37:set ntasks = 16
./run-yellowstone.csh:38:
./run-yellowstone.csh:39:# should be set equal to (CPUs-per-node / tasks_per_node)
./run-yellowstone.csh:40:setenv OMP_NUM_THREADS 2
./run-yellowstone.csh:41:
./run-yellowstone.csh:42:## Do our best to get sufficient stack memory
./run-yellowstone.csh:43:limit stacksize unlimited
./run-yellowstone.csh:44:
./run-yellowstone.csh:45:## Recommended OMP setting from Jim Edwards
./run-yellowstone.csh:46:setenv OMP_STACKSIZE 256M
./run-yellowstone.csh:47:
./run-yellowstone.csh:48:## ROOT OF CAM DISTRIBUTION - probably needs to be customized.
./run-yellowstone.csh:49:## Contains the source code for the CAM distribution.
./run-yellowstone.csh:50:## (the root directory contains the subdirectory "models")
./run-yellowstone.csh:51:set camroot      = /fis/cgd/...
./run-yellowstone.csh:52:
./run-yellowstone.csh:53:## ROOT OF CAM DATA DISTRIBUTION - needs to be customized unless running at NCAR.
./run-yellowstone.csh:54:## Contains the initial and boundary data for the CAM distribution.
./run-yellowstone.csh:55:## (the root directory contains the subdirectories "atm" and "lnd")
./run-yellowstone.csh:56:setenv CSMDATA    $CESMDATAROOT/inputdata/
./run-yellowstone.csh:57:
./run-yellowstone.csh:58:## LOGNAME - used in default settings, must be set if not available
./run-yellowstone.csh:59:## setenv LOGNAME <username>
./run-yellowstone.csh:60:if !($?LOGNAME) then
./run-yellowstone.csh:61:    echo "LOGNAME not available for setting of defaults - setting must be added to this script"
./run-yellowstone.csh:62:    exit 1
./run-yellowstone.csh:63:endif
./run-yellowstone.csh:64:
./run-yellowstone.csh:65:## Default namelist settings:
./run-yellowstone.csh:66:## $case is the case identifier for this run. It will be placed in the namelist.
./run-yellowstone.csh:67:## $runtype is the run type: startup, continue, or branch.
./run-yellowstone.csh:68:## $stop_n is the number of days to integrate (units depend on stop_option)
./run-yellowstone.csh:69:set case         = camrun
./run-yellowstone.csh:70:set runtype      = startup
./run-yellowstone.csh:71:set stop_n       = 1
./run-yellowstone.csh:72:
./run-yellowstone.csh:73:## $wrkdir is a working directory where the model will be built and run.
./run-yellowstone.csh:74:## $blddir is the directory where model will be compiled.
./run-yellowstone.csh:75:## $rundir is the directory where the model will be run.
./run-yellowstone.csh:76:## $cfgdir is the directory containing the CAM configuration scripts.
./run-yellowstone.csh:77:set wrkdir       = /glade/scratch/$LOGNAME
./run-yellowstone.csh:78:set blddir       = $wrkdir/$case/bld
./run-yellowstone.csh:79:set rundir       = $wrkdir/$case
./run-yellowstone.csh:80:set cfgdir       = $camroot/models/atm/cam/bld
./run-yellowstone.csh:81:
./run-yellowstone.csh:82:## Ensure that run and build directories exist
./run-yellowstone.csh:83:mkdir -p $rundir                || echo "cannot create $rundir" && exit 1
./run-yellowstone.csh:84:mkdir -p $blddir                || echo "cannot create $blddir" && exit 1
./run-yellowstone.csh:85:
./run-yellowstone.csh:86:## If an executable doesn't exist, build one.
./run-yellowstone.csh:87:if ( ! -x $blddir/cam && $1 == "build") then
./run-yellowstone.csh:88:    cd $blddir                  || echo "cd $blddir failed" && exit 1
./run-yellowstone.csh:89:    $cfgdir/configure -dyn fv -hgrid 1.9x2.5 -spmd -smp -ntasks $ntasks -nthreads $OMP_NUM_THREADS -cc mpicc -fc mpif90 -fc_type intel  || echo "configure failed" && exit 1
./run-yellowstone.csh:90:    echo "building CAM in $blddir ..."
./run-yellowstone.csh:91:    rm -f Depends
./run-yellowstone.csh:92:    gmake -j8 >&! MAKE.out      || echo "CAM build failed: see $blddir/MAKE.out" && exit 1
./run-yellowstone.csh:93:# Test to see if build was done
./run-yellowstone.csh:94:else if (! -e $blddir/cam) then
./run-yellowstone.csh:95:  echo "ERROR: $blddir/cam does not exists.  You need to run run-yellowstone.sh build."
./run-yellowstone.csh:96:  exit
./run-yellowstone.csh:97:endif
./run-yellowstone.csh:98:
./run-yellowstone.csh:99:## Create the namelist
./run-yellowstone.csh:100:cd $rundir                      || echo "cd $rundir failed" && exit 1
./run-yellowstone.csh:101:$cfgdir/build-namelist -s -config $blddir/config_cache.xml -case $case -runtype $runtype \
./run-yellowstone.csh:102: -namelist "&camexp stop_option='ndays', stop_n=$stop_n /"  || echo "build-namelist failed" && exit 1
./run-yellowstone.csh:103:
./run-yellowstone.csh:104:if ( $1 != "build") then
./run-yellowstone.csh:105:  ## Run CAM - use 'mpirun.lsf' on yellowstone
./run-yellowstone.csh:106:  echo "running CAM in $rundir"
./run-yellowstone.csh:107:  
./run-yellowstone.csh:108:  #yellowstone
./run-yellowstone.csh:109:  mpirun.lsf $blddir/cam    || echo "CAM run failed" && exit 1
./run-yellowstone.csh:110:endif
./run-yellowstone.csh:111:
./run-yellowstone.csh:112:exit 0
