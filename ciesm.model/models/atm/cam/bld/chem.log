./configure:3024:sub absolute_path {
./configure:3025:#
./configure:3026:# Convert a pathname into an absolute pathname, expanding any . or .. characters.
./configure:3027:# Assumes pathnames refer to a local filesystem.
./configure:3028:# Assumes the directory separator is "/".
./configure:3029:#
./configure:3030:  my $path = shift;
./configure:3031:  my $cwd = getcwd();  # current working directory
./configure:3032:  my $abspath;         # resulting absolute pathname
./configure:3033:
./configure:3034:# Strip off any leading or trailing whitespace.  (This pattern won't match if
./configure:3035:# there's embedded whitespace.
./configure:3036:  $path =~ s!^\s*(\S*)\s*$!$1!;
./configure:3037:
./configure:3038:# Convert relative to absolute path.
./configure:3039:
./configure:3040:  if ($path =~ m!^\.$!) {          # path is "."
./configure:3041:      return $cwd;
./configure:3042:  } elsif ($path =~ m!^\./!) {     # path starts with "./"
./configure:3043:      $path =~ s!^\.!$cwd!;
./configure:3044:  } elsif ($path =~ m!^\.\.$!) {   # path is ".."
./configure:3045:      $path = "$cwd/..";
./configure:3046:  } elsif ($path =~ m!^\.\./!) {   # path starts with "../"
./configure:3047:      $path = "$cwd/$path";
./configure:3048:  } elsif ($path =~ m!^[^/]!) {    # path starts with non-slash character
./configure:3049:      $path = "$cwd/$path";
./configure:3050:  }
./configure:3051:
./configure:3052:  my ($dir, @dirs2);
./configure:3053:  my @dirs = split "/", $path, -1;   # The -1 prevents split from stripping trailing nulls
./configure:3054:                                     # This enables correct processing of the input "/".
./configure:3055:
./configure:3056:  # Remove any "" that are not leading.
./configure:3057:  for (my $i=0; $i<=$#dirs; ++$i) {
./configure:3058:      if ($i == 0 or $dirs[$i] ne "") {
./configure:3059:	  push @dirs2, $dirs[$i];
./configure:3060:      }
./configure:3061:  }
./configure:3062:  @dirs = ();
./configure:3063:
./configure:3064:  # Remove any "."
./configure:3065:  foreach $dir (@dirs2) {
./configure:3066:      unless ($dir eq ".") {
./configure:3067:	  push @dirs, $dir;
./configure:3068:      }
./configure:3069:  }
./configure:3070:  @dirs2 = ();
./configure:3071:
./configure:3072:  # Remove the "subdir/.." parts.
./configure:3073:  foreach $dir (@dirs) {
./configure:3074:    if ( $dir !~ /^\.\.$/ ) {
./configure:3075:        push @dirs2, $dir;
./configure:3076:    } else {
./configure:3077:        pop @dirs2;   # remove previous dir when current dir is ..
./configure:3078:    }
./configure:3079:  }
./configure:3080:  if ($#dirs2 == 0 and $dirs2[0] eq "") { return "/"; }
./configure:3081:  $abspath = join '/', @dirs2;
./configure:3082:  return( $abspath );
./configure:3083:}
