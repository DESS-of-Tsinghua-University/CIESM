./configure:2937:
./configure:2938:    # Read the grid parameters from $hgrid_file.
./configure:2939:    my @e = $xml->elements_by_name( "horiz_grid" );
./configure:2940:    my %a = ();
./configure:2941:
./configure:2942:    # Search for matching dycore/grid.
./configure:2943:    my $found = 0;
./configure:2944:  HGRID:
./configure:2945:    while ( my $e = shift @e ) {
./configure:2946:	%a = $e->get_attributes();
./configure:2947:	if ( $dyn_pkg eq $a{'dyn'} and $hgrid eq $a{'hgrid'} ) {
./configure:2948:	    $found = 1;
