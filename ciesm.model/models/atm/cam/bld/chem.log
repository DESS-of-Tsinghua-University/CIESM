./mkDepends:300:
./mkDepends:301:sub find_dependencies {
./mkDepends:302:
./mkDepends:303:    # Find dependencies of input file.
./mkDepends:304:    # Use'd Fortran 90 modules are returned in \@mods.
./mkDepends:305:    # Files that are "#include"d by the cpp preprocessor are returned in \@incs.
./mkDepends:306:
./mkDepends:307:    # Check for circular dependencies in \@mods.  This type of dependency
./mkDepends:308:    # is a consequence of having multiple modules defined in the same file,
./mkDepends:309:    # and having one of those modules depend on the other.
./mkDepends:310:
./mkDepends:311:    my( $file ) = @_;
./mkDepends:312:    my( @mods, @incs );
./mkDepends:313:
./mkDepends:314:    open(FH, $file)  or die "Can't open $file: $!\n";
./mkDepends:315:
./mkDepends:316:    # Construct the makefile target associated with this file.  This is used to
./mkDepends:317:    # check for circular dependencies.
./mkDepends:318:    my ($name, $path, $suffix, $target);
./mkDepends:319:    my @suffixes = ('\.[fF]90', '\.[fF]' );
./mkDepends:320:    ($name, $path, $suffix) = fileparse($file, @suffixes);
./mkDepends:321:    $target = "$name.o";
./mkDepends:322:
./mkDepends:323:    while ( <FH> ) {
./mkDepends:324:	# Search for "#include" and strip filename when found.
./mkDepends:325:	if ( /^#include\s+[<"](.*)[>"]/ ) {
./mkDepends:326:	     push @incs, $1;
./mkDepends:327:	 } 
./mkDepends:328:	# Search for Fortran include dependencies.
./mkDepends:329:	elsif ( /^\s*include\s+['"](.*)['"]/ ) {                   #" for emacs fontlock
./mkDepends:330:	     push @incs, $1;
./mkDepends:331:	}
./mkDepends:332:	# Search for module dependencies.
./mkDepends:333:	elsif ( /^\s*USE(?:\s+|\s*\:\:\s*|\s*,\s*non_intrinsic\s*\:\:\s*)(\w+)/i ) {
./mkDepends:334:	    # Return dependency in the form of a .mod file
./mkDepends:335:	    ($module = $1) =~ tr/A-Z/a-z/;
./mkDepends:336:	    if ( defined $module_files{$module} ) {
./mkDepends:337:		# Check for circular dependency
./mkDepends:338:		unless ("$module_files{$module}.o" eq $target) {
./mkDepends:339:                    $modules_used{$module} = ();
./mkDepends:340:                    push @mods, "$obj_dir".mangle_modfile($module);
./mkDepends:341:		}
./mkDepends:342:	    }
./mkDepends:343:            # If we already have a .mod file around.
./mkDepends:344:            elsif ( defined $trumod_files{$module} ) { 
./mkDepends:345:                push @mods, "$obj_dir".mangle_modfile($trumod_files{$module});
./mkDepends:346:            }
./mkDepends:347:	}
./mkDepends:348:    }
./mkDepends:349:    close( FH );
./mkDepends:350:    return (\@mods, \@incs);
./mkDepends:351:}
./mkDepends:352:
