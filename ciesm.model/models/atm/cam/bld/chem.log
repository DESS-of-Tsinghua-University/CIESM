./perl5lib/Build/Namelist.pm:260:sub delete_variable {
./perl5lib/Build/Namelist.pm:261:
./perl5lib/Build/Namelist.pm:262:# Delete a variable from a specified group
./perl5lib/Build/Namelist.pm:263:
./perl5lib/Build/Namelist.pm:264:  my $self           = shift;
./perl5lib/Build/Namelist.pm:265:  my $group_name     = shift;
./perl5lib/Build/Namelist.pm:266:  my $variable_name  = shift;
./perl5lib/Build/Namelist.pm:267:
./perl5lib/Build/Namelist.pm:268:  my $lc_group = lc($group_name);
./perl5lib/Build/Namelist.pm:269:  my $lc_var   = lc($variable_name);
./perl5lib/Build/Namelist.pm:270:
./perl5lib/Build/Namelist.pm:271:  # Check that the variable is defined.  If not then return error.
./perl5lib/Build/Namelist.pm:272:  if (! defined $self->{$lc_group}->{$lc_var}) {
./perl5lib/Build/Namelist.pm:273:      return -1;
./perl5lib/Build/Namelist.pm:274:  }
./perl5lib/Build/Namelist.pm:275:  else {
./perl5lib/Build/Namelist.pm:276:      # delete the variable
./perl5lib/Build/Namelist.pm:277:      delete $self->{$lc_group}->{$lc_var};
./perl5lib/Build/Namelist.pm:278:  }
./perl5lib/Build/Namelist.pm:279:
./perl5lib/Build/Namelist.pm:280:  # Check whether the group has any other variables.  If not then delete the 
./perl5lib/Build/Namelist.pm:281:  # group as well.
