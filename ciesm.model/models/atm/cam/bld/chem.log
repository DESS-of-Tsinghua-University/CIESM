./perl5lib/XML/Lite.pm:498:sub _dump_tree {
./perl5lib/XML/Lite.pm:499:	my $self = shift;
./perl5lib/XML/Lite.pm:500:	my $node = shift || $self->{tree};
./perl5lib/XML/Lite.pm:501:	
./perl5lib/XML/Lite.pm:502:	my $tree = '';
./perl5lib/XML/Lite.pm:503:	for( my $i = 0; $i < scalar(@$node) && defined $node->[$i]; $i++ ) {
./perl5lib/XML/Lite.pm:504:		if( (scalar(@{$node->[$i]}) == 4) && (defined $node->[$i][2]) ) {
./perl5lib/XML/Lite.pm:505:			$tree .= '[' . join( ',', @{$node->[$i]} ) . "] "
./perl5lib/XML/Lite.pm:506:					. substr($self->{doc}, $node->[$i][0], $node->[$i][1] - $node->[$i][0] + 1) 
./perl5lib/XML/Lite.pm:507:					. "..." 
./perl5lib/XML/Lite.pm:508:					. substr($self->{doc}, $node->[$i][2], $node->[$i][3] - $node->[$i][2] + 1) . " (child $i)\n";
./perl5lib/XML/Lite.pm:509:			# Do child list
./perl5lib/XML/Lite.pm:510:			$i++;
./perl5lib/XML/Lite.pm:511:			$tree .= join( '', map( "  $_\n", split( "\n", $self->_dump_tree( $node->[$i] ) ) ) );
./perl5lib/XML/Lite.pm:512:		} elsif( (scalar(@{$node->[$i]}) == 4) ) {
./perl5lib/XML/Lite.pm:513:			$tree .= '[' . join( ',', $node->[$i][0], $node->[$i][1] ) . "] "
./perl5lib/XML/Lite.pm:514:			      . substr($self->{doc}, $node->[$i][0], $node->[$i][1] - $node->[$i][0] + 1) . "\n";
./perl5lib/XML/Lite.pm:515:		} else {
./perl5lib/XML/Lite.pm:516:			$tree .= "ERROR! Invalid node: [" . join( ',', @{$node->[$i]} ) . "]\n";
./perl5lib/XML/Lite.pm:517:		} # end for
./perl5lib/XML/Lite.pm:518:	} # end for
./perl5lib/XML/Lite.pm:519:
./perl5lib/XML/Lite.pm:520:	return $tree;
./perl5lib/XML/Lite.pm:521:} # end _dump_tree
./perl5lib/XML/Lite.pm:522:
./perl5lib/XML/Lite.pm:523:# module clean-up code here (global destructor)
