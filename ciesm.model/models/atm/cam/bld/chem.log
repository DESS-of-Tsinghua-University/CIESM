./perl5lib/XML/Lite/Element.pm:440:# Sub: _compare_lists
./perl5lib/XML/Lite/Element.pm:441:#
./perl5lib/XML/Lite/Element.pm:442:# Args: $list_ref_1, $list_ref_2
./perl5lib/XML/Lite/Element.pm:443:#
./perl5lib/XML/Lite/Element.pm:444:# Returns: True if the same elements, false otherwise
./perl5lib/XML/Lite/Element.pm:445:#
./perl5lib/XML/Lite/Element.pm:446:# Description: Compare the contents of two lists and returns
./perl5lib/XML/Lite/Element.pm:447:# whether they are the same
./perl5lib/XML/Lite/Element.pm:448:# NOTE: This is a CLASS METHOD (or sub)
./perl5lib/XML/Lite/Element.pm:449:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:450:# Date      Modification                              Author
./perl5lib/XML/Lite/Element.pm:451:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:452:# 06Nov2001 Added to support node lookups                 JW
./perl5lib/XML/Lite/Element.pm:453:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:454:sub _compare_lists {
./perl5lib/XML/Lite/Element.pm:455:	my( $rA, $rB ) = @_;
./perl5lib/XML/Lite/Element.pm:456:	
./perl5lib/XML/Lite/Element.pm:457:	# Lists are not equal unless same size
./perl5lib/XML/Lite/Element.pm:458:	return 0 unless scalar(@$rA) == scalar(@$rB);
./perl5lib/XML/Lite/Element.pm:459:	
./perl5lib/XML/Lite/Element.pm:460:	# Now compare item by item.
./perl5lib/XML/Lite/Element.pm:461:	my $i;
./perl5lib/XML/Lite/Element.pm:462:	for( $i = 0; $i < scalar(@$rA); $i++ ) {
./perl5lib/XML/Lite/Element.pm:463:		return 0 unless $rA->[$i] eq $rB->[$i];
./perl5lib/XML/Lite/Element.pm:464:	} # end for
./perl5lib/XML/Lite/Element.pm:465:	
./perl5lib/XML/Lite/Element.pm:466:	return 1;
./perl5lib/XML/Lite/Element.pm:467:} # end _compare_lists
./perl5lib/XML/Lite/Element.pm:468:
./perl5lib/XML/Lite/Element.pm:469:# module clean-up code here (global destructor)
./perl5lib/XML/Lite/Element.pm:470:END { }
./perl5lib/XML/Lite/Element.pm:471:
./perl5lib/XML/Lite/Element.pm:472:1;  # so the require or use succeeds
./perl5lib/XML/Lite/Element.pm:473:
