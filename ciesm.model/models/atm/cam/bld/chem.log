./run-cray.csh:2:#
./run-cray.csh:3:##=======================================================================
./run-cray.csh:4:##
./run-cray.csh:5:##  run-cray.csh
./run-cray.csh:6:##
./run-cray.csh:7:##  Generic batch submission script for CRAY XT using PBS.  
./run-cray.csh:8:##
./run-cray.csh:9:##-----------------------------------------------------------------------
./run-cray.csh:10:## Usage for PGI compilers (default): 
./run-cray.csh:11:##   qsub run-cray.csh
./run-cray.csh:12:##-----------------------------------------------------------------------
./run-cray.csh:13:##
./run-cray.csh:14:## This is an example script to build and run the default CAM configuration
./run-cray.csh:15:## on a CRAY XT.  The default configuration is 1.9x2.5L26, Finite-Volume 
./run-cray.csh:16:## dynamics, CLM2 land model, CICE ice model, and CAM data ocean model.  
./run-cray.csh:17:## Script will request 8 nodes to run MPI-only, with 32 MPI tasks.
./run-cray.csh:18:##
./run-cray.csh:19:# Name of the queue (CHANGE THIS if needed)
./run-cray.csh:20:#PBS -q debug
./run-cray.csh:21:# Number of procs and walltime (CHANGE THIS if needed)
./run-cray.csh:22:#PBS -l walltime=0:58:00,size=32
./run-cray.csh:23:# output file base name
./run-cray.csh:24:#PBS -N run-cray
./run-cray.csh:25:# Put standard error and standard out in same file
./run-cray.csh:26:#PBS -j oe
./run-cray.csh:27:# Export all Environment variables
./run-cray.csh:28:#PBS -V
./run-cray.csh:29:#PBS -A <please_set>
./run-cray.csh:30:# End of options
./run-cray.csh:31:#=======================================================================
./run-cray.csh:32:
./run-cray.csh:33:source $MODULESHOME/init/csh
./run-cray.csh:34:module load netcdf/3.6.2
./run-cray.csh:35:
./run-cray.csh:36:setenv INC_NETCDF ${NETCDF_DIR}/include
./run-cray.csh:37:setenv LIB_NETCDF ${NETCDF_DIR}/lib
./run-cray.csh:38:
./run-cray.csh:39:setenv MPICH_MAX_SHORT_MSG_SIZE 1024
./run-cray.csh:40:
./run-cray.csh:41:## set this equal to #nodes X #ppn
./run-cray.csh:42:set procs = 32
./run-cray.csh:43:
./run-cray.csh:44:## Do our best to get sufficient stack memory
./run-cray.csh:45:limit stacksize unlimited
./run-cray.csh:46:
./run-cray.csh:47:## ROOT OF CAM DISTRIBUTION - probably needs to be customized.
./run-cray.csh:48:## Contains the source code for the CAM distribution.
./run-cray.csh:49:## (the root directory contains the subdirectory "models")
./run-cray.csh:50:set camroot      = /tmp/work/$LOGNAME/...
./run-cray.csh:51:
./run-cray.csh:52:## ROOT OF CAM DATA DISTRIBUTION
./run-cray.csh:53:## Contains the initial and boundary data for the CAM distribution.
./run-cray.csh:54:## (the root directory contains the subdirectories "atm" and "lnd")
./run-cray.csh:55:setenv CSMDATA     /lustre/widow1/proj/ccsm/inputdata
./run-cray.csh:56:
./run-cray.csh:57:## Default namelist settings:
./run-cray.csh:58:## $case is the case identifier for this run. It will be placed in the namelist.
./run-cray.csh:59:## $runtype is the run type: startup, continue, or branch.
./run-cray.csh:60:## $stop_n is the number of days to integrate (units depends on stop_option)
./run-cray.csh:61:set case         = camrun
./run-cray.csh:62:set runtype      = startup
./run-cray.csh:63:set stop_n       = 1
./run-cray.csh:64:
./run-cray.csh:65:## $wrkdir is a working directory where the model will be built and run.
./run-cray.csh:66:## $blddir is the directory where model will be compiled.
./run-cray.csh:67:## $rundir is the directory where the model will be run.
./run-cray.csh:68:## $cfgdir is the directory containing the CAM configuration scripts.
./run-cray.csh:69:set wrkdir       = /lustre/widow1/scratch/$LOGNAME
./run-cray.csh:70:set blddir       = $wrkdir/$case/bld
./run-cray.csh:71:set rundir       = $wrkdir/$case
./run-cray.csh:72:set cfgdir       = $camroot/models/atm/cam/bld
./run-cray.csh:73:
./run-cray.csh:74:## Ensure that run and build directories exist
./run-cray.csh:75:mkdir -p $rundir                || echo "cannot create $rundir" && exit 1
./run-cray.csh:76:mkdir -p $blddir                || echo "cannot create $blddir" && exit 1
./run-cray.csh:77:
./run-cray.csh:78:## If an executable doesn't exist, build one.
./run-cray.csh:79:if ( ! -x $blddir/cam ) then
./run-cray.csh:80:    cd $blddir                  || echo "cd $blddir failed" && exit 1
./run-cray.csh:81:    $cfgdir/configure -dyn fv -hgrid 1.9x2.5 -spmd -nosmp -ntasks $procs -fc ftn    || echo "configure failed" && exit 1
./run-cray.csh:82:    echo "building CAM in $blddir ..."
./run-cray.csh:83:    rm -f Depends
./run-cray.csh:84:    gmake -j >&! MAKE.out      || echo "CAM build failed: see $blddir/MAKE.out" && exit 1
./run-cray.csh:85:endif
./run-cray.csh:86:
./run-cray.csh:87:## Create the namelist
./run-cray.csh:88:cd $rundir                      || echo "cd $rundir failed" && exit 1
./run-cray.csh:89:$cfgdir/build-namelist -s -config $blddir/config_cache.xml -case $case -runtype $runtype \
./run-cray.csh:90: -namelist "&camexp stop_option='ndays' stop_n=$stop_n  /"  || echo "build-namelist failed" && exit 1
./run-cray.csh:91:
./run-cray.csh:92:## Run CAM
./run-cray.csh:93:echo "running CAM in $rundir"
./run-cray.csh:94:aprun -n $procs $blddir/cam             || echo "CAM run failed" && exit 1
./run-cray.csh:95:
./run-cray.csh:96:exit 0
./run-darwin.csh:1:#! /bin/csh -f
./run-darwin.csh:2:
./run-darwin.csh:3:#-----------------------------------------------------------------------
./run-darwin.csh:4:## Run script for running on a Macintosh OS-X platform (Darwin)
./run-darwin.csh:5:## using the Absoft IBM XLF/XLC compilers. This runs a low resolution
./run-darwin.csh:6:## T5 Eulerian Spectral case in serial mode.
./run-darwin.csh:7:#-----------------------------------------------------------------------
./run-darwin.csh:8:##
./run-darwin.csh:9:
./run-darwin.csh:10:## Do our best to get sufficient stack memory
./run-darwin.csh:11:limit stacksize unlimited
./run-darwin.csh:12:
./run-darwin.csh:13:## netCDF stuff
./run-darwin.csh:14:setenv INC_NETCDF /usr/local/include
./run-darwin.csh:15:setenv LIB_NETCDF /usr/local/lib
./run-darwin.csh:16:
./run-darwin.csh:17:## ROOT OF CAM DISTRIBUTION - probably needs to be customized.
./run-darwin.csh:18:## Contains the source code for the CAM distribution.
./run-darwin.csh:19:## (the root directory contains the subdirectory "models")
./run-darwin.csh:20:set camroot      = $HOME/cam_trunk
./run-darwin.csh:21:
./run-darwin.csh:22:## ROOT OF CAM DATA DISTRIBUTION - needs to be customized unless running at NCAR.
./run-darwin.csh:23:## Contains the initial and boundary data for the CAM distribution.
./run-darwin.csh:24:## (the root directory contains the subdirectories "atm" and "lnd")
./run-darwin.csh:25:setenv CSMDATA    $HOME/inputdata
./run-darwin.csh:26:
./run-darwin.csh:27:## Default namelist settings:
./run-darwin.csh:28:## $case is the case identifier for this run. It will be placed in the namelist.
./run-darwin.csh:29:## $runtype is the run type: startup, continue, or branch.
./run-darwin.csh:30:## $stop_n is the number of timesteps to integrate (units depends on stop_option value)
./run-darwin.csh:31:set dyn          = "eul"
./run-darwin.csh:32:set ocn          = "dom"
./run-darwin.csh:33:set case         = camrun
./run-darwin.csh:34:if ( $dyn != "eul" ) set case = "$case.$dyn"
./run-darwin.csh:35:if ( $ocn != "dom" ) set case = "$case.$ocn"
./run-darwin.csh:36:set runtype      = startup
./run-darwin.csh:37:set stop_n      = 10
./run-darwin.csh:38:
./run-darwin.csh:39:## $wrkdir is a working directory where the model will be built and run.
./run-darwin.csh:40:## $blddir is the directory where model will be compiled.
./run-darwin.csh:41:## $rundir is the directory where the model will be run.
./run-darwin.csh:42:## $cfgdir is the directory containing the CAM configuration scripts.
./run-darwin.csh:43:set wrkdir       = ~/runs/
./run-darwin.csh:44:set blddir       = $wrkdir/$case/bld
./run-darwin.csh:45:set rundir       = $wrkdir/$case
./run-darwin.csh:46:set cfgdir       = $camroot/models/atm/cam/bld
./run-darwin.csh:47:set res          = "8x16"
./run-darwin.csh:48:if ( $dyn == "fv" ) set res = "10x15"
./run-darwin.csh:49:
./run-darwin.csh:50:## Ensure that run and build directories exist
./run-darwin.csh:51:mkdir -p $rundir                || echo "cannot create $rundir" && exit 1
./run-darwin.csh:52:mkdir -p $blddir                || echo "cannot create $blddir" && exit 1
./run-darwin.csh:53:
./run-darwin.csh:54:## build exec
./run-darwin.csh:55:if ( ! -x $blddir/cam )then
./run-darwin.csh:56:    cd $blddir                  || echo "cd $blddir failed" && exit 1
./run-darwin.csh:57:    $cfgdir/configure -test -res $res -ocn $ocn -dyn $dyn -debug  || echo "configure failed" && exit 1
./run-darwin.csh:58:    echo "building CAM in $blddir ..."
./run-darwin.csh:59:    rm -f Depends
./run-darwin.csh:60:    make -j4 >&! MAKE.out      || echo "CAM build failed: see $blddir/MAKE.out" && exit 1
./run-darwin.csh:61:endif
./run-darwin.csh:62:
./run-darwin.csh:63:## Create the namelist
./run-darwin.csh:64:cd $blddir                      || echo "cd $blddir failed" && exit 1
./run-darwin.csh:65:$cfgdir/build-namelist -s -test -case $case -runtype $runtype \
./run-darwin.csh:66: -namelist "&camexp stop_option='nsteps', stop_n=$stop_n, nhtfrq=5/"  || echo "build-namelist failed" && exit 1
./run-darwin.csh:67:
./run-darwin.csh:68:## Run CAM
./run-darwin.csh:69:cd $rundir                      || echo "cd $rundir failed" && exit 1
./run-darwin.csh:70:mv $blddir/*in .
./run-darwin.csh:71:echo "running CAM in $rundir"
./run-darwin.csh:72:$blddir/cam                 || echo "CAM run failed" && exit 1
./run-darwin.csh:73:
./run-darwin.csh:74:exit 0
