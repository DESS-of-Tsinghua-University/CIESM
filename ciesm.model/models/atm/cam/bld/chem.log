./configure:2928:    my $dyn_pkg = $cfg_ref->get('dyn');
./configure:2929:    my $hgrid   = $cfg_ref->get('hgrid');
./configure:2930:
./configure:2931:    # Special case -- If running a PERGRO test with the SLD dycore, use the
./configure:2932:    #                 spectral truncation parameters appropriate for the EUL
./configure:2933:    #                 dycore.  Implement this case by overriding the local
./configure:2934:    #                 value of $dyn_pkg.
./configure:2935:    my $pergro = $cfg_ref->get('pergro');
