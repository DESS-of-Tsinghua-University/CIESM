./perl5lib/XML/Lite/Element.pm:371:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:372:# Sub: _find_self
./perl5lib/XML/Lite/Element.pm:373:#
./perl5lib/XML/Lite/Element.pm:374:# Args: (None)
./perl5lib/XML/Lite/Element.pm:375:#
./perl5lib/XML/Lite/Element.pm:376:# Returns: A reference to our node or undef on error
./perl5lib/XML/Lite/Element.pm:377:#
./perl5lib/XML/Lite/Element.pm:378:# Description: Traverses the owner document's tree to find
./perl5lib/XML/Lite/Element.pm:379:# the node that references the current element. Sets 
./perl5lib/XML/Lite/Element.pm:380:# $self-{self} as a side-effect. Even if this is already set,
./perl5lib/XML/Lite/Element.pm:381:# _find_self will traverse again, so don't call unless needed.
./perl5lib/XML/Lite/Element.pm:382:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:383:# Date      Modification                              Author
./perl5lib/XML/Lite/Element.pm:384:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:385:# 06Nov2001 Added to support children() method            JW
./perl5lib/XML/Lite/Element.pm:386:# 13Mar2002 Check that nodes are defined                  EBK
./perl5lib/XML/Lite/Element.pm:387:# ----------------------------------------------------------
./perl5lib/XML/Lite/Element.pm:388:sub _find_self {
./perl5lib/XML/Lite/Element.pm:389:	my $self = shift;
./perl5lib/XML/Lite/Element.pm:390:	
./perl5lib/XML/Lite/Element.pm:391:	# We actually just call this recusively, so the first 
./perl5lib/XML/Lite/Element.pm:392:	# argument can be a starting point to descend from
./perl5lib/XML/Lite/Element.pm:393:	# but we don't doc that above
./perl5lib/XML/Lite/Element.pm:394:	my $node = shift || $self->{doc}{tree};
./perl5lib/XML/Lite/Element.pm:395:	return undef unless defined $node;
./perl5lib/XML/Lite/Element.pm:396:
./perl5lib/XML/Lite/Element.pm:397:	# Our owner XML::Lite document has a tree (list of lists) that
./perl5lib/XML/Lite/Element.pm:398:	# tracks all elements in the document. Starting at the root
./perl5lib/XML/Lite/Element.pm:399:	# of the tree, walk through each node until we find one with
./perl5lib/XML/Lite/Element.pm:400:	# the same offsets as our $self->{node} has.
./perl5lib/XML/Lite/Element.pm:401:
./perl5lib/XML/Lite/Element.pm:402:	# Walk through the nodes in this node and compare to our selves
./perl5lib/XML/Lite/Element.pm:403:	for( my $i = 0; $i < scalar(@$node) && defined $node->[$i]; $i++ ) {
./perl5lib/XML/Lite/Element.pm:404:
./perl5lib/XML/Lite/Element.pm:405:		# If this is our self, then we're done!
./perl5lib/XML/Lite/Element.pm:406:		# 	NOTE: Since the list references are the same in the by-name hash
./perl5lib/XML/Lite/Element.pm:407:		# 	and tree objects, we can just do a reference compare here.
./perl5lib/XML/Lite/Element.pm:408:		# 	If objects are ever created with non-factory methods then we need to 
./perl5lib/XML/Lite/Element.pm:409:		# 	use a _compare_lists call.
./perl5lib/XML/Lite/Element.pm:410:# 		if( _compare_lists( $node->[$i], $self->{node} ) ) { 
./perl5lib/XML/Lite/Element.pm:411: 		if( $node->[$i] eq $self->{node} ) { 
./perl5lib/XML/Lite/Element.pm:412:			$self->{parent} = $node;
./perl5lib/XML/Lite/Element.pm:413:			$self->{self} = $node->[$i];
./perl5lib/XML/Lite/Element.pm:414:			# If this list has children, then add a pointer to that list
./perl5lib/XML/Lite/Element.pm:415:			$self->{children} = $node->[$i + 1] if (scalar(@{$node->[$i]}) == 4) && (defined $node->[$i][2]);
./perl5lib/XML/Lite/Element.pm:416: 			last;
./perl5lib/XML/Lite/Element.pm:417: 		} # end if
./perl5lib/XML/Lite/Element.pm:418:
./perl5lib/XML/Lite/Element.pm:419:		# For efficiency, we only need look at nodes that start before
./perl5lib/XML/Lite/Element.pm:420:		# our node does
./perl5lib/XML/Lite/Element.pm:421:		if ( defined($node->[$i][0]) && defined($self->{node}->[3]) ) {
./perl5lib/XML/Lite/Element.pm:422:			last if $node->[$i][0] > ($self->{node}->[3] || $self->{node}->[1]);
./perl5lib/XML/Lite/Element.pm:423:		}
./perl5lib/XML/Lite/Element.pm:424:
./perl5lib/XML/Lite/Element.pm:425:		# If this is a node with content (start and end tag) then check children
./perl5lib/XML/Lite/Element.pm:426:		if( (scalar(@{$node->[$i]}) == 4) && (defined $node->[$i][2]) ) {
./perl5lib/XML/Lite/Element.pm:427:			# This is a node with content (start and end tag)
./perl5lib/XML/Lite/Element.pm:428:			# So look at the child node list that follows and see what it's got
./perl5lib/XML/Lite/Element.pm:429: 			$i++;
./perl5lib/XML/Lite/Element.pm:430:			last if defined $self->_find_self( $node->[$i] );
./perl5lib/XML/Lite/Element.pm:431:		} # end for
./perl5lib/XML/Lite/Element.pm:432:
./perl5lib/XML/Lite/Element.pm:433:	} # end for
./perl5lib/XML/Lite/Element.pm:434:
./perl5lib/XML/Lite/Element.pm:435:	# And return it
./perl5lib/XML/Lite/Element.pm:436:	return $self->{self};
./perl5lib/XML/Lite/Element.pm:437:} # end _find_self
./perl5lib/XML/Lite/Element.pm:438:
