./build-namelist:3243:sub create_mode_defs {
./build-namelist:3244:
./build-namelist:3245:# create the list of strings used to define the MAM modes
./build-namelist:3246:
./build-namelist:3247:    my $names     = shift;  # $names->[$m]   - name of mode $m
./build-namelist:3248:    my $types     = shift;  # $types->[$m]   - type of mode $m
./build-namelist:3249:    my $num       = shift;  # $num->[$m]     - name of intersitial number m.r. for mode $m
./build-namelist:3250:    my $num_cw    = shift;  # $num_cw->[$m]  - name of cloud borne number m.r. for mode $m
./build-namelist:3251:    my $num_src   = shift;  # $num_src->[$m] - source of interstitial number m.r. for mode $m
./build-namelist:3252:    my $spec      = shift;  # $spec->[$m][$i]      - name of interstitial species $i in mode $m
./build-namelist:3253:    my $spec_type = shift;  # $spec_type->[$m][$i] - type of species $i in mode $m
./build-namelist:3254:    my $spec_cw   = shift;  # $spec_cw->[$m][$i]   - name of cloud borne species $i in mode $m
./build-namelist:3255:    my $spec_src  = shift;  # $spec_src->[$m][$i]  - source of interstitial species $i in mode $m
./build-namelist:3256:
./build-namelist:3257:    my $mode_def;
./build-namelist:3258:
./build-namelist:3259:    # number of modes
./build-namelist:3260:    my $nmodes = scalar(@$names);
./build-namelist:3261:
./build-namelist:3262:    for (my $m=0; $m < $nmodes; ++$m) {
./build-namelist:3263:
./build-namelist:3264:	# initial mode definition string
./build-namelist:3265:	$mode_def .= "'$names->[$m]:$types->[$m]:=',";
./build-namelist:3266:
./build-namelist:3267:	# add the number mixing ratio field names.  the source of the number conc
./build-namelist:3268:	# of cloud borne aerosol defaults to N
./build-namelist:3269:	$mode_def .= "'$num_src->[$m]:$num->[$m]:N:$num_cw->[$m]:num_mr:+',";
./build-namelist:3270:
./build-namelist:3271:	# number of species in mode
./build-namelist:3272:	my $nspec = scalar(@{$spec->[$m]});
./build-namelist:3273:	for (my $i=0; $i < $nspec; ++$i) {
./build-namelist:3274:
./build-namelist:3275:	    my $term = ":+',";        # more species to come
./build-namelist:3276:	    if ($i == $nspec - 1) {
./build-namelist:3277:		if ($m == $nmodes - 1) {
./build-namelist:3278:		    $term = "'";      # last specie and no more modes
./build-namelist:3279:		}
./build-namelist:3280:		else {
./build-namelist:3281:		    $term = "',";     # last specie but more modes to come
./build-namelist:3282:		}
./build-namelist:3283:	    }
./build-namelist:3284:
./build-namelist:3285:	    # get default properties file
./build-namelist:3286:	    my $spec_name = $spec->[$m][$i];
./build-namelist:3287:	    my $rel_filepath = get_default_value("mam_$spec_name");
./build-namelist:3288:	    my $abs_filepath = set_abs_filepath($rel_filepath, $inputdata_rootdir);
./build-namelist:3289:
./build-namelist:3290: 	    # add species info. the source of the mixing ratio
./build-namelist:3291:            # for cloud borne species defaults to N
./build-namelist:3292:	    $mode_def .= "'" .
./build-namelist:3293:		         $spec_src->[$m][$i]  . ":" . 
./build-namelist:3294:		         $spec->[$m][$i]      . ":N:" . 
./build-namelist:3295:		         $spec_cw->[$m][$i]   . ":" . 
./build-namelist:3296:		         $spec_type->[$m][$i] . ":" . 
./build-namelist:3297:		         $abs_filepath        . $term;
./build-namelist:3298:	}
./build-namelist:3299:
./build-namelist:3300:
./build-namelist:3301:    }
./build-namelist:3302:
./build-namelist:3303:    return $mode_def;
./build-namelist:3304:}
