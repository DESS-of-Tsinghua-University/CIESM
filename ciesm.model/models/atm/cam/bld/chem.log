./run-scam.csh:48:setenv LIB_NETCDF ${NCHOME}/lib
./run-scam.csh:49:setenv NCARG_ROOT /contrib/ncarg
./run-scam.csh:50:setenv PATH ${NCHOME}/bin:${FC_DIR}/bin:${NCARG_ROOT}/bin:${PATH}
./run-scam.csh:51:setenv LD_LIBRARY_PATH ${FC_DIR}/lib:${LIB_NETCDF}:${LD_LIBRARY_PATH}
./run-scam.csh:52:
./run-scam.csh:53:alias MATH 'set \!:1 = `echo "\!:3-$" | bc -l`'  # do not modify this
