./build-namelist:3745:sub absolute_path {
./build-namelist:3746:#
./build-namelist:3747:# Convert a pathname into an absolute pathname, expanding any . or .. characters.
./build-namelist:3748:# Assumes pathnames refer to a local filesystem.
./build-namelist:3749:# Assumes the directory separator is "/".
./build-namelist:3750:#
./build-namelist:3751:  my $path = shift;
./build-namelist:3752:  my $cwd = getcwd();  # current working directory
./build-namelist:3753:  my $abspath;         # resulting absolute pathname
./build-namelist:3754:
./build-namelist:3755:# Strip off any leading or trailing whitespace.  (This pattern won't match if
./build-namelist:3756:# there's embedded whitespace.
./build-namelist:3757:  $path =~ s!^\s*(\S*)\s*$!$1!;
./build-namelist:3758:
./build-namelist:3759:# Convert relative to absolute path.
./build-namelist:3760:
./build-namelist:3761:  if ($path =~ m!^\.$!) {          # path is "."
./build-namelist:3762:      return $cwd;
./build-namelist:3763:  } elsif ($path =~ m!^\./!) {     # path starts with "./"
./build-namelist:3764:      $path =~ s!^\.!$cwd!;
./build-namelist:3765:  } elsif ($path =~ m!^\.\.$!) {   # path is ".."
./build-namelist:3766:      $path = "$cwd/..";
./build-namelist:3767:  } elsif ($path =~ m!^\.\./!) {   # path starts with "../"
./build-namelist:3768:      $path = "$cwd/$path";
./build-namelist:3769:  } elsif ($path =~ m!^[^/]!) {    # path starts with non-slash character
./build-namelist:3770:      $path = "$cwd/$path";
./build-namelist:3771:  }
./build-namelist:3772:
./build-namelist:3773:  my ($dir, @dirs2);
./build-namelist:3774:  my @dirs = split "/", $path, -1;   # The -1 prevents split from stripping trailing nulls
./build-namelist:3775:                                     # This enables correct processing of the input "/".
./build-namelist:3776:
./build-namelist:3777:  # Remove any "" that are not leading.
./build-namelist:3778:  for (my $i=0; $i<=$#dirs; ++$i) {
./build-namelist:3779:      if ($i == 0 or $dirs[$i] ne "") {
./build-namelist:3780:	  push @dirs2, $dirs[$i];
./build-namelist:3781:      }
./build-namelist:3782:  }
./build-namelist:3783:  @dirs = ();
./build-namelist:3784:
./build-namelist:3785:  # Remove any "."
./build-namelist:3786:  foreach $dir (@dirs2) {
./build-namelist:3787:      unless ($dir eq ".") {
./build-namelist:3788:	  push @dirs, $dir;
./build-namelist:3789:      }
./build-namelist:3790:  }
./build-namelist:3791:  @dirs2 = ();
./build-namelist:3792:
./build-namelist:3793:  # Remove the "subdir/.." parts.
./build-namelist:3794:  foreach $dir (@dirs) {
./build-namelist:3795:    if ( $dir !~ /\.\./ ) {
./build-namelist:3796:        push @dirs2, $dir;
./build-namelist:3797:    } else {
./build-namelist:3798:        pop @dirs2;   # remove previous dir when current dir is ..
./build-namelist:3799:    }
./build-namelist:3800:  }
./build-namelist:3801:  if ($#dirs2 == 0 and $dirs2[0] eq "") { return "/"; }
./build-namelist:3802:  $abspath = join '/', @dirs2;
./build-namelist:3803:  return( $abspath );
./build-namelist:3804:}
