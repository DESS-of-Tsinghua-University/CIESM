./perl5lib/XML/Lite/Element.pm:142:sub get_content {
./perl5lib/XML/Lite/Element.pm:143:	my $self = shift;
./perl5lib/XML/Lite/Element.pm:144:
./perl5lib/XML/Lite/Element.pm:145:	# If we don't have any content, then we should return 
./perl5lib/XML/Lite/Element.pm:146:	# '' right away.
./perl5lib/XML/Lite/Element.pm:147:	return '' unless defined $self->{node}[2];
./perl5lib/XML/Lite/Element.pm:148:	
./perl5lib/XML/Lite/Element.pm:149:	# Using our pointers, find everything between our tags
./perl5lib/XML/Lite/Element.pm:150:	my $content = substr( $self->{doc}{doc}, $self->{node}[1] + 1, $self->{node}[2] - $self->{node}[1] - 1 );
./perl5lib/XML/Lite/Element.pm:151:	
./perl5lib/XML/Lite/Element.pm:152:	# Now, restore any CDATA chunks that may have been pulled out
./perl5lib/XML/Lite/Element.pm:153:	$content =~ s/<!\[CDATA\[(\S+)\s*\]\]\/>/<![CDATA[$self->{doc}{_CDATA}[$1]]]>/g;
./perl5lib/XML/Lite/Element.pm:154:	
./perl5lib/XML/Lite/Element.pm:155:	# And return the content
./perl5lib/XML/Lite/Element.pm:156:	return $content;
./perl5lib/XML/Lite/Element.pm:157:} # end get_content
