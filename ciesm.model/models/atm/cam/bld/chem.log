./get_nxny.pl:71:sub absolute_path {
./get_nxny.pl:72:#
./get_nxny.pl:73:# Convert a pathname into an absolute pathname, expanding any . or .. characters.
./get_nxny.pl:74:# Assumes pathnames refer to a local filesystem.
./get_nxny.pl:75:# Assumes the directory separator is "/".
./get_nxny.pl:76:#
./get_nxny.pl:77:  my $path = shift;
./get_nxny.pl:78:  my $cwd = getcwd();  # current working directory
./get_nxny.pl:79:  my $abspath;         # resulting absolute pathname
./get_nxny.pl:80:
./get_nxny.pl:81:# Strip off any leading or trailing whitespace.  (This pattern won't match if
./get_nxny.pl:82:# there's embedded whitespace.
./get_nxny.pl:83:  $path =~ s!^\s*(\S*)\s*$!$1!;
./get_nxny.pl:84:
./get_nxny.pl:85:# Convert relative to absolute path.
./get_nxny.pl:86:
./get_nxny.pl:87:  if ($path =~ m!^\.$!) {          # path is "."
./get_nxny.pl:88:      return $cwd;
./get_nxny.pl:89:  } elsif ($path =~ m!^\./!) {     # path starts with "./"
./get_nxny.pl:90:      $path =~ s!^\.!$cwd!;
./get_nxny.pl:91:  } elsif ($path =~ m!^\.\.$!) {   # path is ".."
./get_nxny.pl:92:      $path = "$cwd/..";
./get_nxny.pl:93:  } elsif ($path =~ m!^\.\./!) {   # path starts with "../"
./get_nxny.pl:94:      $path = "$cwd/$path";
./get_nxny.pl:95:  } elsif ($path =~ m!^[^/]!) {    # path starts with non-slash character
./get_nxny.pl:96:      $path = "$cwd/$path";
./get_nxny.pl:97:  }
./get_nxny.pl:98:
./get_nxny.pl:99:  my ($dir, @dirs2);
./get_nxny.pl:100:  my @dirs = split "/", $path, -1;   # The -1 prevents split from stripping trailing nulls
./get_nxny.pl:101:                                     # This enables correct processing of the input "/".
./get_nxny.pl:102:
./get_nxny.pl:103:  # Remove any "" that are not leading.
./get_nxny.pl:104:  for (my $i=0; $i<=$#dirs; ++$i) {
./get_nxny.pl:105:      if ($i == 0 or $dirs[$i] ne "") {
./get_nxny.pl:106:          push @dirs2, $dirs[$i];
./get_nxny.pl:107:      }
./get_nxny.pl:108:  }
./get_nxny.pl:109:  @dirs = ();
./get_nxny.pl:110:
./get_nxny.pl:111:  # Remove any "."
./get_nxny.pl:112:  foreach $dir (@dirs2) {
./get_nxny.pl:113:      unless ($dir eq ".") {
./get_nxny.pl:114:          push @dirs, $dir;
./get_nxny.pl:115:      }
./get_nxny.pl:116:  }
./get_nxny.pl:117:  @dirs2 = ();
./get_nxny.pl:118:
./get_nxny.pl:119:  # Remove the "subdir/.." parts.
./get_nxny.pl:120:  foreach $dir (@dirs) {
./get_nxny.pl:121:    if ( $dir !~ /^\.\.$/ ) {
./get_nxny.pl:122:        push @dirs2, $dir;
./get_nxny.pl:123:    } else {
./get_nxny.pl:124:        pop @dirs2;   # remove previous dir when current dir is ..
./get_nxny.pl:125:    }
./get_nxny.pl:126:  }
./get_nxny.pl:127:  if ($#dirs2 == 0 and $dirs2[0] eq "") { return "/"; }
./get_nxny.pl:128:  $abspath = join '/', @dirs2;
./get_nxny.pl:129:  return( $abspath );
./get_nxny.pl:130:}
