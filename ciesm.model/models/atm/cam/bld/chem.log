./perl5lib/Build/NamelistDefaults.pm:319:       if (defined $elements{$name}) {
./perl5lib/Build/NamelistDefaults.pm:320:	   push @{$elements{$name}}, $e;
./perl5lib/Build/NamelistDefaults.pm:321:       }
./perl5lib/Build/NamelistDefaults.pm:322:       else {
./perl5lib/Build/NamelistDefaults.pm:323:	   $elements{$name} = [$e];
./perl5lib/Build/NamelistDefaults.pm:324:       }
./perl5lib/Build/NamelistDefaults.pm:325:    }
./perl5lib/Build/NamelistDefaults.pm:326:
./perl5lib/Build/NamelistDefaults.pm:327:    $self->{'names'}    = \%names;
./perl5lib/Build/NamelistDefaults.pm:328:    $self->{'elements'} = \%elements;
