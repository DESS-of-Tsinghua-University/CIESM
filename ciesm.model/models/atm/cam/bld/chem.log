./run-scam.csh:60:
./run-scam.csh:61:foreach iopname ($IOP_CASES_TO_RUN) # change this, see above (depending on case you want to simulate)
./run-scam.csh:62:
./run-scam.csh:63:foreach tarray ($CAM_TIMESTEPS_TO_RUN)    # change this, the host model timestep 
./run-scam.csh:64:
./run-scam.csh:65:foreach levarr ($CAM_LEVELS_TO_RUN)      # change this, number of levels to run
./run-scam.csh:66:
./run-scam.csh:67:set EXPNAME={$CASE}_{$iopname}_L{$levarr}_T{$tarray}
./run-scam.csh:68:
./run-scam.csh:69:#########################################################################
./run-scam.csh:70:### Set some case specific parameters here
