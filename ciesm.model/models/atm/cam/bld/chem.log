./build-namelist:3843:	$old = $opts->{$opt};
./build-namelist:3844:	$opts->{$opt} = valid_option($old, @expect)
./build-namelist:3845:	    or die "$ProgName - ERROR: invalid value of $opt ($old) specified in $source\n".
./build-namelist:3846:                   "expected one of: @expect\n";
./build-namelist:3847:    }
./build-namelist:3848:
./build-namelist:3849:}
