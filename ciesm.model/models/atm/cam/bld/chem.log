./mkDepends:354:
./mkDepends:355:sub find_file {
./mkDepends:356:
./mkDepends:357:# Search for the specified file in the list of directories in the global
./mkDepends:358:# array @file_paths.  Return the first occurance found, or the null string if
./mkDepends:359:# the file is not found.
./mkDepends:360:
./mkDepends:361:    my($file) = @_;
./mkDepends:362:    my($dir, $fname);
./mkDepends:363:
./mkDepends:364:    foreach $dir (@file_paths) {
./mkDepends:365:	$fname = "$dir/$file";
./mkDepends:366:	if ( -f  $fname ) { return $fname; }
./mkDepends:367:    }
./mkDepends:368:    return '';  # file not found
./mkDepends:369:}
./mkDepends:370:
./mkDepends:371:#--------------------------------------------------------------------------------------
./mkDepends:372:
./mkDepends:373:sub rm_duplicates {
./mkDepends:374:
./mkDepends:375:# Return a list with duplicates removed.
./mkDepends:376:
./mkDepends:377:    my ($in) = @_;       # input arrary reference
./mkDepends:378:    my @out = ();
./mkDepends:379:    my $i;
./mkDepends:380:    my %h = ();
./mkDepends:381:    foreach $i (@$in) {
./mkDepends:382:	$h{$i} = '';
./mkDepends:383:    }
./mkDepends:384:    @out = keys %h;
./mkDepends:385:    return \@out;
./mkDepends:386:}
./mkDepends:387:
./mkDepends:388:#--------------------------------------------------------------------------------------
./mkDepends:389:
./mkDepends:390:sub mangle_modfile {
./mkDepends:391:
./mkDepends:392:# Return the name of the module file corresponding
./mkDepends:393:# to a given module.
./mkDepends:394:
./mkDepends:395:    my ($mod) = @_;
./mkDepends:396:    my $fname;
./mkDepends:397:
./mkDepends:398:    if ($mangle_scheme eq "lower") {
./mkDepends:399:        ($fname = $mod) =~ tr/A-Z/a-z/;
./mkDepends:400:        $fname .= ".mod";
./mkDepends:401:    } elsif ($mangle_scheme eq "upper") {
./mkDepends:402:        ($fname = $mod) =~ tr/a-z/A-Z/;
./mkDepends:403:        $fname .= ".MOD";
./mkDepends:404:    } else {
./mkDepends:405:        die "Unrecognized mangle_scheme!\n";
./mkDepends:406:    }
./mkDepends:407:
./mkDepends:408:    return $fname;
./mkDepends:409:
./mkDepends:410:}
