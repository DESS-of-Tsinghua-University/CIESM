./configure:3089:# Substitute for any environment variables contained in a pathname.
./configure:3090:# Assumes the directory separator is "/".
./configure:3091:#
./configure:3092:  my $path = shift;
./configure:3093:  my $newpath;         # resulting pathname
./configure:3094:
./configure:3095:# Strip off any leading or trailing whitespace.  (This pattern won't match if
./configure:3096:# there's embedded whitespace.
./configure:3097:  $path =~ s!^\s*(\S*)\s*$!$1!;
./configure:3098:
./configure:3099:  my ($dir, @dirs2);
./configure:3100:  my @dirs = split "/", $path, -1;   # The -1 prevents split from stripping trailing nulls
./configure:3101:                                     # This enables correct processing of the input "/".
./configure:3102:
./configure:3103:  foreach $dir (@dirs) {
./configure:3104:    if ( $dir =~ /^\$(.+)$/ ) {
