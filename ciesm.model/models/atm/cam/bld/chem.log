./perl5lib/XML/Lite/Element.pm:72:sub new {
./perl5lib/XML/Lite/Element.pm:73:	my $self = {};
./perl5lib/XML/Lite/Element.pm:74:	my $proto = shift;
./perl5lib/XML/Lite/Element.pm:75:	my $class = ref($proto) || $proto;
./perl5lib/XML/Lite/Element.pm:76:
./perl5lib/XML/Lite/Element.pm:77:	# The arguments are as follows:
./perl5lib/XML/Lite/Element.pm:78:	#   $owner_document   is an XML::Lite object within which this element lives
./perl5lib/XML/Lite/Element.pm:79:	#   \@pointers        is a two or four element array ref containing the offsets
./perl5lib/XML/Lite/Element.pm:80:	#                     into the original document of the start and end points of 
./perl5lib/XML/Lite/Element.pm:81:	#                     the opening and closing (when it exists) tags for the element
./perl5lib/XML/Lite/Element.pm:82:	
./perl5lib/XML/Lite/Element.pm:83:	# Validate arguments
./perl5lib/XML/Lite/Element.pm:84:	return undef unless @_ >= 2;
./perl5lib/XML/Lite/Element.pm:85:	return undef unless ref($_[0]) && (ref($_[1]) eq 'ARRAY');
./perl5lib/XML/Lite/Element.pm:86:	
./perl5lib/XML/Lite/Element.pm:87:	# Load 'em up
./perl5lib/XML/Lite/Element.pm:88:	
./perl5lib/XML/Lite/Element.pm:89:	# The data structure for the ::Element object has these properties
./perl5lib/XML/Lite/Element.pm:90:	#   doc               A reference to the containing XML::Lite object
./perl5lib/XML/Lite/Element.pm:91:	#   node              A reference to an array of pointers to our element in the document
./perl5lib/XML/Lite/Element.pm:92:	#   self              A pointer to our own entry in the owner doc's tree
./perl5lib/XML/Lite/Element.pm:93:	#   parent            A pointer to our parent elemenet's entry in the owner doc's tree
./perl5lib/XML/Lite/Element.pm:94:	#   name              The name on our tag
./perl5lib/XML/Lite/Element.pm:95:	#   _attrs            A string of the attibutes in our tag (unparsed)
./perl5lib/XML/Lite/Element.pm:96:	#  attrs              A hash ref of attributes in our tag
./perl5lib/XML/Lite/Element.pm:97:	
./perl5lib/XML/Lite/Element.pm:98:	$self->{doc} = $_[0];
./perl5lib/XML/Lite/Element.pm:99:	$self->{node} = $_[1];
./perl5lib/XML/Lite/Element.pm:100:	
./perl5lib/XML/Lite/Element.pm:101:	# Using the pointers, find out tag name, and attribute list from the 
./perl5lib/XML/Lite/Element.pm:102:	# opening tag (if there are any attributes).
./perl5lib/XML/Lite/Element.pm:103:	my $tag = substr( $self->{doc}{doc}, $self->{node}[0], $self->{node}[1] - $self->{node}[0] + 1 );
./perl5lib/XML/Lite/Element.pm:104:	if( $tag =~ m{^<\s*([^/>\s]+)\s+([^>]+)\s*/?\s*>$} ) {
./perl5lib/XML/Lite/Element.pm:105:		$self->{name} = $1;
./perl5lib/XML/Lite/Element.pm:106:		$self->{_attrs} = $2;		# Store the attributes as a scalar. Parse when asked
./perl5lib/XML/Lite/Element.pm:107:	} elsif( $tag =~ m{^<\s*([^/>\s]+)\s*/?\s*>$} ) {
./perl5lib/XML/Lite/Element.pm:108:		$self->{name} = $1;
./perl5lib/XML/Lite/Element.pm:109:		$self->{_attrs} = '';
./perl5lib/XML/Lite/Element.pm:110:	} else {
./perl5lib/XML/Lite/Element.pm:111:		# Should have been caught in the parsing! maybe an assert?
./perl5lib/XML/Lite/Element.pm:112:		$self->{doc}->_error( 'ELM_NOT_CLOSED', $self->{node}[0] + $self->{doc}->{doc_offset} );
./perl5lib/XML/Lite/Element.pm:113:	} # end if
./perl5lib/XML/Lite/Element.pm:114:	
./perl5lib/XML/Lite/Element.pm:115:	# Good. Now returns it.
./perl5lib/XML/Lite/Element.pm:116:	bless ($self, $class);
./perl5lib/XML/Lite/Element.pm:117:	return $self;
./perl5lib/XML/Lite/Element.pm:118:} # end new
